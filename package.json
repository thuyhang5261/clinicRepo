{
  "name": "clinic-livestream",
  "version": "1.0.0",
  "description": "Clinic LiveStream Application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "docker:build": "docker build -t clinic-livestream .",
    "docker:run": "docker run -p 3000:3000 -p 1935:1935 -p 8000:8000 clinic-livestream",
    "docker:compose": "docker-compose up -d",
    "pm2:start": "pm2 start server.js --name clinic-livestream",
    "pm2:stop": "pm2 stop clinic-livestream",
    "pm2:restart": "pm2 restart clinic-livestream",
    "pm2:logs": "pm2 logs clinic-livestream"
  },
  "keywords": ["livestream", "webrtc", "rtmp", "clinic"],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "node-media-server": "^2.6.3",
    "socket.io": "^4.7.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.5.0",
    "eslint": "^8.44.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
